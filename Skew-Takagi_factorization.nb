8a955a321ae8"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
    "The", " ", "function", " ", "generating", " ", "a", " ", "random", " ", 
     "skew"}], "-", 
    RowBox[{"symmetric", " ", "matrix"}]}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"RandomSkewSym", "[", "n_", "]"}], ":=", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"#", " ", "-", " ", 
         RowBox[{"Transpose", "[", "#", "]"}]}], ")"}], "&"}], "@", 
      RowBox[{"RandomComplex", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"-", "1"}], "-", "\[ImaginaryI]"}], ",", 
          RowBox[{"1", "+", "\[ImaginaryI]"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"n", ",", "n"}], "}"}]}], "]"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Single", "-", 
     RowBox[{"run", " ", "testing"}]}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"n", ",", "U", ",", "\[CapitalLambda]", ",", "A"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"n", "=", "10"}], ";", "\[IndentingNewLine]", 
      RowBox[{"A", "=", 
       RowBox[{"RandomSkewSym", "[", "n", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"\[CapitalLambda]", ",", "U"}], "}"}], "=", 
       RowBox[{"SkewTakagi", "[", "A", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Skewsymmetric: \>\"", ",", 
        RowBox[{"AntisymmetricMatrixQ", "[", "A", "]"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Unitarity: \>\"", ",", 
        RowBox[{"UnitaryMatrixQ", "[", "U", "]"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Diagonality: \>\"", ",", 
        RowBox[{"DiagonalMatrixQ", "[", "\[CapitalLambda]", "]"}]}], "]"}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Error of skew-Takagi: \>\"", ",", 
        RowBox[{"Norm", "[", 
         RowBox[{
          RowBox[{"A", "-", 
           RowBox[{"U", ".", "\[CapitalLambda]", ".", 
            RowBox[{"J", "[", "n", "]"}], ".", 
            RowBox[{"Transpose", "[", "U", "]"}]}]}], ",", 
          "\"\<Frobenius\>\""}], "]"}]}], "]"}], ";"}]}], 
    "\[IndentingNewLine]", "]"}]}]}]], "Input",
