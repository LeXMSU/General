Cell["The algorithm", "Subsection",
 CellChangeTimes->{{3.8229132368999476`*^9, 
  3.8229132409149504`*^9}},ExpressionUUID->"f7d638e6-e602-4827-8bb7-\
98b09e991ea8"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"Skew", "-", 
    RowBox[{"Takagi", " ", "factorization"}]}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"J", "[", "n_", "]"}], ":=", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"EvenQ", "[", "n", "]"}], ",", 
       RowBox[{"KroneckerProduct", "[", 
        RowBox[{
         RowBox[{"IdentityMatrix", "[", 
          FractionBox["n", "2"], "]"}], ",", 
         RowBox[{"(", GridBox[{
            {"0", "1"},
            {
             RowBox[{"-", "1"}], "0"}
           }], ")"}]}], "]"}], ",", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"n", "\[Equal]", "1"}], ",", 
         RowBox[{"{", 
          RowBox[{"{", "1", "}"}], "}"}], ",", 
         RowBox[{"ArrayFlatten", "[", 
          RowBox[{"(", GridBox[{
             {
              RowBox[{"KroneckerProduct", "[", 
               RowBox[{
                RowBox[{"IdentityMatrix", "[", 
                 FractionBox[
                  RowBox[{"n", "-", "1"}], "2"], "]"}], ",", 
                RowBox[{"(", GridBox[{
                   {"0", "1"},
                   {
                    RowBox[{"-", "1"}], "0"}
                  }], ")"}]}], "]"}], "0"},
             {"0", "1"}
            }], ")"}], "]"}]}], "]"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"SkewTakagi", "[", "A_", "]"}], ":=", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"U", ",", "\[CapitalLambda]", ",", "V"}], "}"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"U", ",", "\[CapitalLambda]", ",", "V"}], "}"}], "=", 
         RowBox[{"SingularValueDecomposition", "[", "A", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"\[CapitalLambda]", ",", 
          RowBox[{"U", ".", 
           RowBox[{"MatrixPower", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"ConjugateTranspose", "[", "U", "]"}], ".", 
              RowBox[{"Conjugate", "[", "V", "]"}], ".", 
              RowBox[{"J", "[", 
               RowBox[{"Length", "[", "A", "]"}], "]"}]}], ",", 
             FractionBox["1", "2"]}], "]"}]}]}], "}"}]}]}], 
      "\[IndentingNewLine]", "\[IndentingNewLine]", "]"}]}], ";"}], 
   "\[IndentingNewLine]"}]}]], "Input",
 CellChangeTimes->{{3.8229119325745687`*^9, 3.82291196083096*^9}, 
   3.822913070298127*^9, 3.822913407799116*^9},
 CellLabel->"In[79]:=",ExpressionUUID->"2683a355-f4f4-4315-868e-2814c6ab5e2f"]
}, Open  ]],

