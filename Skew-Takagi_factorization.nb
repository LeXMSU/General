Cell[CellGroupData[{

Cell["\<\
Implementation of fermionic creation  - annihilation operators and quadratic \
Hamiltonians\
\>", "Item",
 CellChangeTimes->{{3.822924214346381*^9, 3.8229242518261995`*^9}, {
  3.82292429182706*^9, 
  3.8229243035216837`*^9}},ExpressionUUID->"2414218d-4b50-4b7f-830f-\
77b6a9e11ddb"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"\[Sigma]3", "=", 
   RowBox[{"(", GridBox[{
      {"1", "0"},
      {"0", 
       RowBox[{"-", "1"}]}
     }], ")"}]}], ";", 
  RowBox[{"\[Sigma]p", "=", 
   RowBox[{"(", GridBox[{
      {"0", "1"},
      {"0", "0"}
     }], ")"}]}], ";", 
  RowBox[{"\[Sigma]m", "=", 
   RowBox[{"(", GridBox[{
      {"0", "0"},
      {"1", "0"}
     }], ")"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"fun1", "[", "k_", "]"}], "/;", 
    RowBox[{"(", 
     RowBox[{"k", "\[GreaterEqual]", "2"}], ")"}]}], ":=", 
   RowBox[{"Nest", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"KroneckerProduct", "[", 
       RowBox[{"\[Sigma]3", ",", "#"}], "]"}], "&"}], ",", "\[Sigma]3", ",", 
     RowBox[{"k", "-", "2"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"fun1", "[", "1", "]"}], "=", 
   RowBox[{"{", 
    RowBox[{"{", "1", "}"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"cm", "[", 
    RowBox[{"n_", ",", "k_"}], "]"}], ":=", 
   RowBox[{"Fold", "[", 
    RowBox[{"KroneckerProduct", ",", 
     RowBox[{"fun1", "[", "k", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Sigma]m", ",", 
       RowBox[{"IdentityMatrix", "[", 
        SuperscriptBox["2", 
         RowBox[{"n", "-", "k"}]], "]"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"cp", "[", 
    RowBox[{"n_", ",", "k_"}], "]"}], ":=", 
   RowBox[{"Fold", "[", 
    RowBox[{"KroneckerProduct", ",", 
     RowBox[{"fun1", "[", "k", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Sigma]p", ",", 
       RowBox[{"IdentityMatrix", "[", 
        SuperscriptBox["2", 
         RowBox[{"n", "-", "k"}]], "]"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Ham", "[", "A_", "]"}], ":=", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      FractionBox["\[ImaginaryI]", "2"], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"A", "[", 
          RowBox[{"[", 
           RowBox[{"i", ",", "j"}], "]"}], "]"}], " ", 
         RowBox[{
          RowBox[{"cp", "[", 
           RowBox[{
            RowBox[{"Length", "[", "A", "]"}], ",", "i"}], "]"}], ".", 
          RowBox[{"cp", "[", 
           RowBox[{
            RowBox[{"Length", "[", "A", "]"}], ",", "j"}], "]"}]}]}], "+", 
        RowBox[{
         RowBox[{"Conjugate", "[", 
          RowBox[{"A", "[", 
           RowBox[{"[", 
            RowBox[{"i", ",", "j"}], "]"}], "]"}], "]"}], " ", 
         RowBox[{
          RowBox[{"cm", "[", 
           RowBox[{
            RowBox[{"Length", "[", "A", "]"}], ",", "i"}], "]"}], ".", 
          RowBox[{"cm", "[", 
           RowBox[{
            RowBox[{"Length", "[", "A", "]"}], ",", "j"}], "]"}]}]}]}], 
       ")"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"Length", "[", "A", "]"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "1", ",", 
       RowBox[{"Length", "[", "A", "]"}]}], "}"}]}], "]"}]}], ";"}]}], "Input",
 CellLabel->"In[69]:=",ExpressionUUID->"851385c1-eb36-4314-8a09-a61c20f2b219"]
}, Open  ]],

Cell[CellGroupData[{

Cell["\<\
Algorithms for Heisenberg evolution based on skew-Takagi factorization and on \
direct computaion of matrix exponential\
\>", "Item",
 CellChangeTimes->{{3.8229243677623158`*^9, 3.8229244796265507`*^9}, {
  3.8229245615689993`*^9, 
  3.822924563784964*^9}},ExpressionUUID->"5c935a26-4d59-44a8-acc2-\
16c15d5db52d"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"CtHeisenberg", "[", 
    RowBox[{"A_", ",", "t_"}], "]"}], ":=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"MatrixExp", "[", 
       RowBox[{"\[ImaginaryI]", " ", 
        RowBox[{"Ham", "[", "A", "]"}], "t"}], "]"}], ".", 
      RowBox[{"cm", "[", 
       RowBox[{
        RowBox[{"Length", "[", "A", "]"}], ",", "i"}], "]"}], ".", 
      RowBox[{"MatrixExp", "[", 
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", 
        RowBox[{"Ham", "[", "A", "]"}], "t"}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"Length", "[", "A", "]"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"SkewTakagiBasedCtHeisenberg", "[", 
   RowBox[{"A_", ",", "t_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"n", ",", "\[CapitalLambda]", ",", " ", "U"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"\[CapitalLambda]", ",", " ", "U"}], "}"}], "=", 
      RowBox[{"SkewTakagi", "[", "A", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"n", "=", 
      RowBox[{"Length", "[", "A", "]"}]}], ";", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         SubsuperscriptBox["\[Sum]", 
          RowBox[{"j", "=", "1"}], "n"], 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"U", ".", 
             RowBox[{"MatrixFunction", "[", 
              RowBox[{"Cos", ",", 
               RowBox[{"\[CapitalLambda]", " ", "t"}]}], "]"}], ".", 
             RowBox[{"ConjugateTranspose", "[", "U", "]"}]}], ")"}], "[", 
           RowBox[{"[", 
            RowBox[{"i", ",", "j"}], "]"}], "]"}], 
          RowBox[{"cm", "[", 
           RowBox[{"n", ",", "j"}], "]"}]}]}], "+", " ", 
        RowBox[{
         SubsuperscriptBox["\[Sum]", 
          RowBox[{"j", "=", "1"}], "n"], 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"U", ".", 
             RowBox[{"MatrixFunction", "[", 
              RowBox[{"Sin", ",", 
               RowBox[{"\[CapitalLambda]", " ", "t"}]}], "]"}], ".", 
             RowBox[{"J", "[", "n", "]"}], ".", 
             RowBox[{"Transpose", "[", "U", "]"}]}], ")"}], "[", 
           RowBox[{"[", 
            RowBox[{"i", ",", "j"}], "]"}], "]"}], 
